/**
 *  FullSubtractor
 *  input a, b ,inborrow 
 *  output outsub,outborrow
 */

CHIP FullSubtractor {
		IN a, b, inborrow;
		OUT outsub, outborrow;
		
    PARTS:
		HalfSubtractor(a=a ,b=b ,sub=w1 , borrow=w2 );
		HalfSubtractor(a=w1 ,b=inborrow ,sub=outsub , borrow=w3);
		Or(a=w3 ,b=w2 ,out=outborrow );
	
}
